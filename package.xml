<?xml version="1.0" encoding="ISO-8859-1" ?>
<package version="1.0">
 <name>ingres</name>
 <summary>Extension for CA's Ingres database.</summary>
 <description>
  This extension supports Computer Associates's Ingres Relational Database.
 </description>
 <license>PHP</license>
 <maintainers>
  <maintainer>
   <user>grant</user>
   <name>Grant Croker</name>
   <email>grantc@php.net</email>
   <role>lead</role>
  </maintainer>
 </maintainers>
 <release>
  <version>1.0</version>
  <date>2005-05-25</date>
  <state>beta</state>
  <notes>
   Initial Release
  </notes>
  <configureoptions>
   <configureoption name="with-ingres" prompt="Ingres II_SYSTEM dir?" />
  </configureoptions>
  <filelist>
   <dir name="/" baseinstalldir="ingres">
    <file role="src">ii.c</file>
    <file role="src">ii.h</file>
    <file role="src">php_ii.h</file>
    <file role="src">config.m4</file>
    <file role="src">config.w32</file>
    <file role="doc">README</file>
   </dir>
  </filelist>
 </release>
 <changelog>
  <release>
   <version>1.0</version>
   <date>2005-05-25</date>
   <state>beta</state>
   <notes>
    Initial release.
   </notes>
  </release>
 </changelog>
</package>
